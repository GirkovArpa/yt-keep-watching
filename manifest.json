{
  "manifest_version": 2,
  "name": "YT Keep Watching",
  "permissions": [
    "activeTab"
  ],
  "version": "0.0.0.1",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_title": "YT Keep Watching"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/watch?v=*"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ]
}